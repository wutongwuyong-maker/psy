# 环境配置示例文件
# 复制此文件为 .env 并修改相应配置

# 安全配置
SECRET_KEY=your-secret-key-here-must-be-long-and-random
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 数据库配置
DATABASE_URL_NO_DB=mysql+pymysql://root:password@localhost:3306/
DB_NAME=psy_test_db

# 日志配置
LOG_LEVEL=INFO

# CORS配置
ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080

# 报告目录配置
REPORT_DIR=reports

# 文件上传限制
MAX_FILE_SIZE_MB=10

